{
    "$id": "https://raw.githubusercontent.com/mkawahara22/hypar-schemas/main/CellProps.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "Map for Assigning Properties to cells",
    "title": "CellProps",
    "x-namespace": "Elements",
    "type": ["object","null"],
    "allOf": [{"$ref": "https://hypar.io/Schemas/GeometricElement.json"}],
    "required": ["Cell"],
    "properties": {
        "UserCells":{
            "type":"array",
            "items": {
                "type":"object",
                "required" : ["Cell_ID","JoistSpacing"],
                "properties": {
                    "Cell_ID": {
                        "description": "Collection of All Cells and Associated properties assigned by the user",
                        "type":"string"
                    },
                    "JoistSpacing":{
                        "type" : "number"
                    },
                    "Loads":{
                        "type":"array",
                        "items": {
                            "type":"object",
                            "properties": {
                                "name":{
                                    "type":"string"
                                },
                                "pressure":{
                                    "type":"number"
                                }
                            }
                        }
                    }
                }
            }
        },
        "VerticalEdges":{
            "type":"array",
            "items": {
                "type":"object",
                "properties": {
                    "id":{
                        "type":"number"
                    }
                }
            }
        }
    },
    "additionalProperties": false
}