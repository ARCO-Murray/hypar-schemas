{
  "$id": "https://raw.githubusercontent.com/mkawahara22/hypar-schemas/main/AME_Inputs/WindData.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Wind Data",
  "title": "WindData",
  "x-namespace": "Elements",
  "type": ["object", "null"],
  "allOf": [{ "$ref": "https://hypar.io/Schemas/Element.json" }],
  "properties": {
      "UltimateWindSpeed": {
      "description": "Ultimate wind speed in mph",
      "type": "number"
    },
    "SDLResistingUplift": {
      "description": "Percentage of the SDL resisting wind uplift",
      "type": "number"
    },
    "EnclosureClass": {
      "description": "Enclosure Class per ASCE",
      "type": "string"
    },
    "ExposureCategory": {
      "description": "Exposure Category per ASCE",
      "type": "string"
    },
    "Topographic Factor": {
      "description": "Topographic Factor per ASCE, conservatively taken as 1.0",
      "type": "number"
    },
    "Directionality Factor": {
      "description": "Directionality Factor per ASCE, typically 0.85 for buildings",
      "type": "number"
    }
  }
}
