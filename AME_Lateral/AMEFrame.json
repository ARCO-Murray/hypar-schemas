{
  "$id": "https://raw.githubusercontent.com/mkawahara22/hypar-schemas/main/AME_Lateral/AMEFrame.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "An object that describes a lateral frame",
  "title": "AMEFrame",
  "x-namespace": "Elements",
  "type": ["object", "null"],
  "allOf": [{ "$ref": "https://hypar.io/Schemas/Element.json" }],
  "properties": {
    "FrameId": {
      "type": "string",
      "format": "uuid"
    },
    "FrameName": {
      "type": "string",
      "default": ""
    },
    "FrameFootprintPolygon": {
      "$ref": "https://hypar.io/Schemas/Geometry/Polygon.json",
      "description": "Encapsulating polygon used to capture any columns that fall within it."
    },
    "FrameType": {
      "type": "string",
      "enum": ["Moment", "Braced", "Raised_WP", "Single_Braced"]
    },
    "Nodes": {
      "type": "array",
      "items": {
        "$ref": "https://hypar.io/Schemas/Geometry/Vector3.json"
      }
    },
    "Members": {
      "type": "array",
      "items": {
        "$ref": "https://hypar.io/Schemas/GeometricElement.json"
      }
    },
    "FrameTopOfSteelLine": {
      "$ref": "https://hypar.io/Schemas/Geometry/Polyline.json",
      "description": "Line representing top of steel of the frame line (useful for modeling beams after design)."
    },
    "FrameFootprintLine": {
      "$ref": "https://hypar.io/Schemas/Geometry/Line.json",
      "description": "The whole lineâ€”represents the expansion joint."
    },
    "MainGrid": {
      "$ref": "https://raw.githubusercontent.com/mkawahara22/hypar-schemas/main/Grid_ID.json"
    },
    "StartGrid": {
      "$ref": "https://raw.githubusercontent.com/mkawahara22/hypar-schemas/main/Grid_ID.json"
    },
    "EndGrid": {
      "$ref": "https://raw.githubusercontent.com/mkawahara22/hypar-schemas/main/Grid_ID.json"
    },
    "Reaction_k": {
      "type": "number",
      "default": 0
    },
    "ColumnLines": {
      "type": "array",
      "items": {
        "$ref": "https://hypar.io/Schemas/Geometry/Line.json"
      }
    },
    "BeamLines": {
      "type": "array",
      "items": {
        "$ref": "https://hypar.io/Schemas/Geometry/Line.json"
      }
    },
    "BraceLines": {
      "type": "array",
      "items": {
        "$ref": "https://hypar.io/Schemas/Geometry/Line.json"
      }
    },
    "RaisedWorkpointHeight_ft": {
      "type": "number",
      "default": 15,
      "description": "Height where the brace attaches to the frame (if raised WP)."
    }
  },
  "additionalProperties": false
}
