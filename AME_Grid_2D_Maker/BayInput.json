{
  "$id": "https://raw.githubusercontent.com/mkawahara22/hypar-schemas/main/AME_Grid_2D_Maker/BayInput.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Input Instance for a Bay",
  "title": "BayInput",
  "x-namespace": "Elements",
  "type": ["object", "null"],
  "properties": {
    "NumberOfBays": {
      "description": "Number of bays",
      "type": "integer",
      "$hyparOrder": 0
    },
    "Length_ft": {
      "description": "Bay length in ft",
      "type": "number",
      "$hyparOrder": 1
    },
    "DimensionString": {
      "type": "string",
      "description": "Dimension of the bay in the following format example: 54'-6 3/8\"",
      "$hyparOrder": 2
    },
    "UseSpeedBaySlope": {
      "description": "Flag to use the speedbay slope value if true",
      "type": "boolean",
      "$hyparOrder": 3
    }
  },
  "additionalProperties": false
}
