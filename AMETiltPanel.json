{
    "$id": "https://raw.githubusercontent.com/mkawahara22/hypar-schemas/main/AMETiltPanel.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "A tilt or precast wall panel",
    "title": "AMETiltPanel",
    "x-namespace": "Elements",
    "type": ["object", "null"],
    "allOf": [{ "$ref": "https://hypar.io/Schemas/GeometricElement.json" }],
    "required": [
        "Polygon",
        "Thickness_in"
    ],
    "properties": {
        "Polygon": {
            "description": "the plane for the roof slope.",
            "$ref": "https://hypar.io/Schemas/Geometry/Polygon.json"
        },
        "Total_Height_ft": {
            "description": "Overall height of the panel",
            "type": "number"
        },
        "AboveFF_Height_ft": {
            "description": "Depth panel extends below the ground",
            "type": "number"
        },
        "BelowFF_Height_ft": {
            "description": "Depth panel extends below the ground",
            "type": "number"
        },
        "Length_ft": {
            "description": "Length of the panel in plan view",
            "type": "number"
        },
        "Gross_Area_ft2": {
            "description": "Gross area of the panel",
            "type": "number"
        },
        "Thickness_in": {
            "description": "Thickness of the panel",
            "type": "number"
        },
        "AvgParapet_height_ft": {
            "description": "Average additional height above roof",
            "type": "number"
        },
        "TypVReinf_Size": {
            "description": "Average additional height above roof",
            "type": "number"
        },
        "TypVReinf_Spacing_in": {
            "description": "Average additional height above roof",
            "type": "number"
        },
        "TypVReinf_Tag": {
            "description": "Average additional height above roof",
            "type": "string"
        },
        "TypHReinf_Size": {
            "description": "Average additional height above roof",
            "type": "number"
        },
        "TypHReinf_Spacing_in": {
            "description": "Average additional height above roof",
            "type": "number"
        },
        "TypHReinf_Tag": {
            "description": "Average additional height above roof",
            "type": "string"
        }
    },
    "additionalProperties": false
}