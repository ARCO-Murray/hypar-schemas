{
  "$id": "https://raw.githubusercontent.com/mkawahara22/hypar-schemas/main/AME_Summary/LoadingZone.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Information about gravity loading zone.",
  "title": "LoadingZone",
  "x-namespace": "Elements",
  "type": ["object", "null"],
  "allOf": [{ "$ref": "https://hypar.io/Schemas/Element.json" }],
  "properties": {
    "LoadingProfile": {
      "type": "array",
      "description": "Polygon and voids of the given loading zone",
      "$ref": "https://hypar.io/Schemas/Geometry/Profile.json"
    },
    "ZoneLoads": {
      "type": "array",
      "description": "List of unique loads that apply to this loading zone",
      "items": {
        "$ref": "https://raw.githubusercontent.com/mkawahara22/hypar-schemas/main/AMELoad.json"
      }
    }
  },
  "additionalProperties": false
}
